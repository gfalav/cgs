<h1>Lista de Proyectos</h1>

<table>
  <tr>
    <th class="thclass">Proyecto</th>
    <th class="thclass">Comitente</th>
    <th class="thclass">Fproyecto</th>
    <th class="thclass">Ubicacion</th>
    <th class="thclass">Expediente</th>
  </tr>

<% @proyectos.each do |proyecto| %>
  <tr>
    <td class="tdclass"><%= proyecto.proyecto %></td>
    <td class="tdclass"><%= proyecto.comitente %></td>
    <td class="tdclass"><%= proyecto.fproyecto %></td>
    <td class="tdclass"><%= proyecto.ubicacion %></td>
    <td class="tdclass"><%= proyecto.expediente %></td>
    <td class="tbclass"><%= link_to 'Show', proyecto %></td>
    <td class="tbclass"><%= link_to 'Edit', edit_proyecto_path(proyecto) %></td>
    <td class="tbclass"><%= link_to 'Destroy', proyecto, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Proyecto', new_proyecto_path %>
